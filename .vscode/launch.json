  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension with careerx-web",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
